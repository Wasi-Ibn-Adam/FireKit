<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/easy_logo"
        android:label="AuthKit"
        android:roundIcon="@drawable/easy_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestKotlin"
        tools:targetApi="31">
        <activity
            android:name=".activities.Act"
            android:exported="true"
            android:theme="@style/Theme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Test" android:exported="false"/>
    </application>

</manifest>